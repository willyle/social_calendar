<%= form_for @profile, url: '/profile/update', method: "post" do |f| %>
	<%= f.label :first_name, "First Name " %>
	<%= f.text_field :first_name %>
	<br>
	<%= f.label :last_name, "Last Name " %>
	<%= f.text_field :last_name %>
	<br>
	<%= f.label :gender, "Gender " %>
	<%= f.select :gender, [['Male','Male'],['Female','female']], include_blank: true %>
	<br>
	<%= f.label :email, "email " %>
	<%= f.email_field :email %>
	<br>
	<%= f.label :birthday, "Birthday " %>
	<%= f.date_field :birthday %>
	<br>
	<%= f.label :work, "Work " %>
	<%= f.text_field :work %>
	<br>
	<%= f.label :skills, "Skills " %>
	<br>
	<%= f.text_area :skills %>
	<br>
	<%= f.label :interests, "Interests " %><br>
	<%= f.text_area :interests %>
	<br><br>
	<%= f.submit "Update Profile" %>
<% end %>